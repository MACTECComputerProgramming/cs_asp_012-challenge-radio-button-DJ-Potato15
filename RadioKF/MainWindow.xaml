        xmlns:local="clr-namespace:RadioKF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Pencil" Content="Pencil" HorizontalAlignment="Left" Margin="110,52,0,0" VerticalAlignment="Top" Width="104" Click="Pencil_Click"/>
        <Button x:Name="Pen" Content="Pen" HorizontalAlignment="Left" Height="19" Margin="110,97,0,0" VerticalAlignment="Top" Width="104" Click="Pen_Click"/>
        <Button x:Name="Phone" Content="Phone" HorizontalAlignment="Left" Height="21" Margin="110,139,0,0" VerticalAlignment="Top" Width="104" Click="Phone_Click"/>
        <Button x:Name="Pad" Content="Pad" HorizontalAlignment="Left" Height="20" Margin="110,183,0,0" VerticalAlignment="Top" Width="104" Click="Pad_Click"/>

    </Grid>
</Window>
